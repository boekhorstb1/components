{
    "name": "horde/components",
    "description": "Developer tool for managing Horde components",
    "type": "horde-application",
    "homepage": "https://www.horde.org",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Jan Schneider",
            "email": "jan@horde.org",
            "role": "lead"
        },
        {
            "name": "Gunnar Wrobel",
            "email": "p@rdus.de",
            "role": "lead"
        },
        {
            "name": "Ralf Lang",
            "email": "lang@b1-systems.de",
            "role": "contributor"
        }
    ],
    "version": "1.0.0alpha2",
    "time": "2021-02-24",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com/"
        }
    ],
    "require": {
        "horde/horde-installer-plugin": "*",
        "php": "^7",
        "horde/Autoloader": "*",
        "horde/Argv": "*",
        "horde/Cli": "^2.3",
        "horde/Cli_Modular": "^2.1",
        "horde/Http": "*",
        "horde/Pear": "*",
        "horde/Role": "*",
        "horde/Text_Diff": "*",
        "horde/Util": "*",
        "horde/Injector": "*",
        "horde/Release": "*",
        "horde/Yaml": "*"
    },
    "suggest": {
        "horde/test": "3",
        "squizlabs/php_codesniffer": "^3.5",
        "pear/archive_tar": "^1.4",
        "sebastian/phpcpd": "^6",
        "sebastian/phpdcd": "^1",
        "phploc/phploc": "^7",
        "phpmd/phpmd": "^2.9",
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-0": {
            "Components": "lib/"
        },
        "psr-4": {
            "Horde\\Components\\": "src/"
        }
    },
    "bin": [
        "bin/horde-components"
    ]
}