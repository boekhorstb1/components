        },
        {
            "url": "https://pear.phpmd.org",
            "type": "pear"
        }
    ],
    "require": {
        "horde/horde-installer-plugin": "*",
        "php": "^7",
        "horde/autoloader": "*",
        "horde/argv": "*",
        "horde/cli": "^2.3",
        "horde/cli_modular": "^2.1",
        "horde/http": "*",
        "horde/pear": "*",
        "horde/role": "*",
        "horde/text_diff": "*",
        "horde/util": "*",
        "horde/injector": "*",
        "horde/release": "*",
        "horde/yaml": "*"
    },
    "suggest": {
        "horde/test": "*",
        "pear-pear.php.net/Archive_Tar": "*",
        "pear-pear.php.net/PHP_CodeSniffer": "*",
        "pear-pear.phpunit.de/PHPUnit": "*",
        "pear-pear.phpunit.de/phpcpd": "*",
        "pear-pear.phpunit.de/phpdcd": "*",
        "pear-pear.phpunit.de/phploc": "*",
        "pear-pear.phpmd.org/PHP_PMD": "*",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-0": {
            "Components": "lib/"
        },
        "psr-4": {
            "Horde\\Components\\": "src/"
        }
    },
    "bin": [
        "bin/horde-components"
    ]
}